{
  "name": "ai-bookmarks",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@9.0.0",
  "scripts": {
    "build": "react-router build",
    "cf-typegen": "wrangler types",
    "deploy": "pnpm run build && wrangler deploy",
    "dev": "react-router dev",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "wrangler d1 migrations apply ai-bookmarks-db --local",
    "db:migrate:prod": "wrangler d1 migrations apply ai-bookmarks-db --remote",
    "db:studio": "drizzle-kit studio",
    "postinstall": "pnpm run cf-typegen",
    "preview": "pnpm run build && vite preview",
    "typecheck": "pnpm run cf-typegen && react-router typegen && tsc -b",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "setup:env": "cp .dev.vars.example .dev.vars"
  },
  "dependencies": {
    "arctic": "^3.7.0",
    "better-auth": "^1.4.10",
    "isbot": "^5.1.31",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router": "^7.10.0",
    "sanitize-html": "^2.17.0"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.13.5",
    "@eslint/js": "^9.39.2",
    "@react-router/dev": "^7.10.0",
    "@tailwindcss/vite": "^4.1.13",
    "@types/node": "^22",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@types/sanitize-html": "^2.16.0",
    "drizzle-kit": "^0.31.8",
    "drizzle-orm": "^0.45.1",
    "eslint": "^9.39.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.26",
    "prettier": "^3.7.4",
    "tailwindcss": "^4.1.13",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.51.0",
    "vite": "^7.1.7",
    "vite-tsconfig-paths": "^5.1.4",
    "wrangler": "^4.54.0"
  }
}
