{
  "name": "ai-bookmarks",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@9.0.0",
  "scripts": {
    "build": "react-router build",
    "cf-typegen": "wrangler types",
    "deploy": "pnpm run build && wrangler deploy",
    "dev": "react-router dev",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "wrangler d1 migrations apply ai-bookmarks-db --local",
    "db:migrate:prod": "wrangler d1 migrations apply ai-bookmarks-db",
    "db:studio": "drizzle-kit studio",
    "postinstall": "pnpm run cf-typegen",
    "preview": "pnpm run build && vite preview",
    "typecheck": "pnpm run cf-typegen && react-router typegen && tsc -b"
  },
  "dependencies": {
    "@cloudflare/ai": "^1.1.4",
    "drizzle-orm": "^0.37.0",
    "isbot": "^5.1.31",
    "isomorphic-dompurify": "^2.35.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router": "^7.10.0"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.13.5",
    "@react-router/dev": "^7.10.0",
    "@tailwindcss/vite": "^4.1.13",
    "@types/node": "^22",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "drizzle-kit": "^0.30.0",
    "tailwindcss": "^4.1.13",
    "typescript": "^5.9.2",
    "vite": "^7.1.7",
    "vite-tsconfig-paths": "^5.1.4",
    "wrangler": "^4.54.0"
  }
}
